nome = input('digite seu nome:')
nome1 = input('digite seu nome:')
n1 = float(input('Digite seu salario:'))
n2 = float(input('Digite seu salario:'))

sub = n1 - n2

if(n1 > n2):
    print(nome,'Tem',sub,'de valor a mais que vc!')
elif(n2 > n1):
    print(nome1,'tem',sub,'de valor a mais que v!')
else:
    print('Seus salarios são iguai')
