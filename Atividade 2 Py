n1 = float(input('ensira numero:'))
n2 = float(input('ensira numero:'))

if(n1 > n2):
    print('n1 é maior que n2')
elif(n2 > n1):
    print('n2 é maior que n1')
else:
    print('Esses numeros são iguais')

